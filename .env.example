# Server
PORT=3000
NODE_ENV=production

# Database
MONGODB_URI=mongodb+srv://username:password@cluster.mongodb.net/youthalive?retryWrites=true&w=majority

# Authentication
JWT_SECRET=your-jwt-secret-key
JWT_EXPIRES_IN=7d
REGISTRATION_SECRET=your-shared-secret-key-for-signup

# Jotform
JOTFORM_API_KEY=your-jotform-api-key

# Email (Gmail SMTP)
GMAIL_USER=your-gmail-address@gmail.com
GMAIL_PASSWORD=your-gmail-app-password
FROM_EMAIL=your-gmail-address@gmail.com
FROM_NAME=Youth Alive SA

# Frontend URL (for CORS)
FRONTEND_URL=https://your-frontend-app.vercel.app

# Backend URL (for webhook configuration)
BACKEND_URL=https://your-backend-app.herokuapp.com
